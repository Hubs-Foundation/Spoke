    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "tap-xunit": "^2.4.1",
    "terser-webpack-plugin": "^2.0.1",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
