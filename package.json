    "fast-deep-equal": "^2.0.1",
    "fs-extra": "^6.0.1",
    "gltf-unlit-generator": "^0.5.1",
    "koa": "^2.5.1",
    "koa-body": "^4.0.3",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
