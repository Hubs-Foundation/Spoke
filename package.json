  "license": "MPL-2.0",
  "version": "0.7.7",
  "engines": {
    "node": ">=10",
    "npm": ">=6.4"
  },
  "bin": {
    "spoke": "bin/spoke"
  },
  "main": "lib/server/index.js",
  "files": [
    "bin/",
    "lib/",
    "public/",
    "LICENSE"
