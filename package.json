    "opn": "^5.3.0",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react": "^16.4.0",
    "react-color": "^2.14.1",
    "react-contextmenu": "^2.9.2",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.4.0",
    "react-hotkeys": "^1.1.4",
    "react-modal": "^3.4.4",
    "react-mosaic-component": "^1.1.1",
    "react-select": "^1.2.1",
