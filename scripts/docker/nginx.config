server {
    listen 8080 ssl;
    ssl_certificate /ssl/cert;
    ssl_certificate_key /ssl/key;
    location / {
        root /www;
        autoindex off;
        add_header 'Access-Control-Allow-Origin' '*';
    }
}

server {
    listen 1111;
    location =/_healthz {
        root /www/_healthz.html;
    }    
}
